{
  "name": "RESTful Students",
  "dockerComposeFile": "docker-compose.yml",
  "service": "app",
  "runServices": [
    "app",
    "db-postgres",
    "db-mysql"
  ],
  "workspaceFolder": "/workspace",
  "settings": {
    "sqltools.connections": [
      {
        "previewLimit": 50,
        "server": "db-postgres",
        "port": 5432,
        "driver": "PostgreSQL",
        "name": "student-pg",
        "group": "restfull-students",
        "database": "STUDENT",
        "username": "postgres",
        "password": "postgres"
      },
      {
        "previewLimit": 50,
        "server": "db-mysql",
        "port": 3306,
        "driver": "MySQL",
        "name": "student-mysql",
        "group": "restfull-students",
        "database": "STUDENT",
        "username": "mysql",
        "password": "mysql"
      },
      {
        "previewLimit": 50,
        "driver": "SQLite",
        "name": "student-sqlite",
        "group": "restfull-students",
        "database": "./sqlite/student.sqlite"
      },
    ],
    "sqltools.useNodeRuntime": true
  },
  "extensions": [
    "dbaeumer.vscode-eslint",
    "esbenp.prettier-vscode",
    "ms-vscode.vscode-typescript-next",
    "mtxr.sqltools",
    "mtxr.sqltools-driver-pg",
    "mtxr.sqltools-driver-sqlite",
    "mtxr.sqltools-driver-mysql"
  ],
  "remoteUser": "node",
  "shutdownAction": "stopCompose"
}